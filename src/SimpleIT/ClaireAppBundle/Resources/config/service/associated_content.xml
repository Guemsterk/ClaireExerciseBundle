            <call method="setTagRepository">
                <argument type="service" id="simple_it.claire.associated_content.repository.tag"/>
            </call>
            <call method="setTagByCategoryRepository">
                <argument type="service" id="simple_it.claire.associated_content.repository.tag_by_category"/>
            </call>
            <call method="setRecommendedCourseByTagRepository">
                <argument type="service" id="simple_it.claire.associated_content.repository.recommended_course_by_tag"/>
