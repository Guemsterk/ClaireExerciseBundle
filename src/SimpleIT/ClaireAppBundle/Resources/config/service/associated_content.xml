        <parameter key="simple_it.claire.associated_content.repository.tag.class">SimpleIT\ClaireAppBundle\Repository\AssociatedContent\TagRepository</parameter>
        <parameter key="simple_it.claire.associated_content.repository.tag_by_category.class">SimpleIT\ClaireAppBundle\Repository\AssociatedContent\TagByCategoryRepository</parameter>
