        </service>
        <service id="simple_it.claire.associated_content.category" class="%simple_it.claire.associated_content.category.class%">
            <call method="setCategoryRepository">
                <argument type="service" id="simple_it.claire.associated_content.repository.category"/>
            </call>
            <call method="setCourseByCategoryRepository">
                <argument type="service" id="simple_it.claire.associated_content.repository.course_by_category"/>
