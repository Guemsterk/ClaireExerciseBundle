<?xml version="1.0" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="oc.claire.use_cases.associated_content.category_by_course.get_draft_course_category.class">OC\CLAIRE\BusinessRules\UseCases\AssociatedContent\CategoryByCourse\GetDraftCourseCategory</parameter>
        <parameter key="oc.claire.use_cases.associated_content.category_by_course.save_course_category.class">OC\CLAIRE\BusinessRules\UseCases\AssociatedContent\CategoryByCourse\SaveCourseCategory</parameter>
    </parameters>
    <services>
        <service id="oc.claire.use_cases.associated_content.category_by_course.get_draft_course_category" class="%oc.claire.use_cases.associated_content.category_by_course.get_draft_course_category.class%">
            <call method="setCategoryByCourseGateway">
                <argument type="service" id="oc.claire.associated_content.repository.category_by_course"/>
            </call>
        </service>
        <service id="oc.claire.use_cases.associated_content.category_by_course.save_course_category" class="%oc.claire.use_cases.associated_content.category_by_course.save_course_category.class%">
            <call method="setGetCourseStatuses">
                <argument type="service" id="oc.claire.use_cases.course.course.get_course_statuses"/>
            </call>
            <call method="setCategoryByCourseGateway">
                <argument type="service" id="oc.claire.associated_content.repository.category_by_course"/>
            </call>
        </service>
    </services>
</container>
