<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="SimpleIT\ExerciseBundle\Entity\ExerciseModel\Metadata" table="claire_exercise_model_metadata" repository-class="SimpleIT\ExerciseBundle\Repository\ExerciseModel\MetadataRepository">

        <id name="ownerExerciseModel" association-key="true"/>
        <id name="key" type="string" column="meta_key"/>
        <field name="value" column="meta_value" type="string" nullable="false"/>

        <many-to-one target-entity="OwnerExerciseModel" field="ownerExerciseModel">
            <join-column name="owner_exercise_model_id" referenced-column-name="id"/>
        </many-to-one>

    </entity>
</doctrine-mapping>
