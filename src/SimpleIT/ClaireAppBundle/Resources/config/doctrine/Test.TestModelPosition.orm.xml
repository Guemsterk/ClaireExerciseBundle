<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                                      http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="SimpleIT\ExerciseBundle\Entity\Test\TestModelPosition" table="claire_exercise_test_model_position" repository-class="SimpleIT\ExerciseBundle\Repository\Test\TestModelPositionRepository">

        <id name="testModel" association-key="true"/>
        <id name="ownerExerciseModel" association-key="true"/>

        <field name="position" column="position" type="integer" nullable="false"/>

        <many-to-one target-entity="SimpleIT\ExerciseBundle\Entity\Test\TestModel" field="testModel">
            <join-column name="test_model_id" referenced-column-name="id" on-delete="CASCADE"/>
        </many-to-one>

        <many-to-one target-entity="SimpleIT\ExerciseBundle\Entity\ExerciseModel\OwnerExerciseModel" field="ownerExerciseModel">
            <join-column name="owner_exercise_model_id" referenced-column-name="id" on-delete="CASCADE"/>
        </many-to-one>

    </entity>
</doctrine-mapping>
