<?xml version="1.0" ?>

<container xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://symfony.com/schema/dic/services" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <parameters>
        <parameter key="serializer.class">OC\ClaireAppBundle\Util\Dummy</parameter>
        <parameter key="logger.class">OC\ClaireAppBundle\Util\Dummy</parameter>
        <parameter key="controller_name_converter.class">OC\ClaireAppBundle\Util\Dummy</parameter>
        <parameter key="buzz.message_factory.class">OC\ClaireAppBundle\Util\Dummy</parameter>
    </parameters>
    <services>
        <service id="serializer" class="%serializer.class%"/>
        <service id="logger" class="%logger.class%"/>
        <service id="controller_name_converter" class="%controller_name_converter.class%"/>
        <service id="buzz.message_factory" class="%buzz.message_factory.class%"/>
    </services>
</container>
